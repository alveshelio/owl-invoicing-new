{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "description": "Owl Invoicing",
  "scripts": {
    "dev": "solid-start dev",
    "build": "tsc && solid-start build",
    "start": "solid-start start",
    "test": "vitest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint-fix": "eslint --ext .ts,.tsx ./src --fix",
    "format": "prettier --write ./src --ignore-path .gitignore",
    "codegen": "gql-gen --config codegen.ts"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@solidjs/testing-library": "0.7.1",
    "@storybook/addon-actions": "7.0.22",
    "@storybook/addon-backgrounds": "7.0.22",
    "@storybook/addon-docs": "7.0.22",
    "@storybook/addon-essentials": "7.0.22",
    "@storybook/addon-interactions": "7.0.22",
    "@storybook/addon-links": "7.0.22",
    "@storybook/addon-measure": "7.0.22",
    "@storybook/addon-outline": "7.0.22",
    "@storybook/addons": "7.0.22",
    "@storybook/builder-vite": "7.0.22",
    "@storybook/channel-postmessage": "7.0.22",
    "@storybook/channel-websocket": "7.0.22",
    "@storybook/client-api": "7.0.22",
    "@storybook/html": "7.0.22",
    "@storybook/preview-web": "7.0.22",
    "@storybook/testing-library": "0.1.0",
    "@tanstack/solid-query": "4.29.14",
    "@testing-library/jest-dom": "5.16.5",
    "@typescript-eslint/parser": "5.59.11",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-solid": "0.12.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "graphql-codegen-typescript-common": "0.18.2",
    "graphql-request": "^5.2.0",
    "jsdom": "22.1.0",
    "msw": "1.2.2",
    "msw-storybook-addon": "1.8.0",
    "postcss": "8.4.24",
    "solid-start-node": "0.2.26",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "0.32.2",
    "whatwg-fetch": "3.6.2"
  },
  "dependencies": {
    "@solidjs/meta": "0.28.5",
    "@solidjs/router": "0.8.2",
    "graphql": "^16.6.0",
    "solid-js": "1.7.6",
    "solid-start": "0.2.26",
    "undici": "5.22.1"
  },
  "engines": {
    "node": ">=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "webpack",
        "@storybook/*",
        "@types/react",
        "eslint"
      ]
    }
  },
  "npm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "webpack",
        "@storybook/*"
      ]
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}